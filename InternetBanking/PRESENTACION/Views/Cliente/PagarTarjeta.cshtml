
@{
    ViewBag.Title = "PagarTarjeta";
}

<h2>PagarTarjeta</h2>

<form action="PagarTarjeta" method="post" enctype="multipart/form-data">
    <p>Desde: <input type="text" name="cuenta" size="40"></p>
    <p>Monto a pagar: <input type="text" name="monto"></p>
    <p>
        <input type="submit" value="Enviar">
        <input type="reset" value="Borrar">
        <a class="btn btn-primary" href="/Cliente/Tarjeta">Atras</a>
    </p>
</form>